{
  "name": "enhanced-vscode-nvm",
  "displayName": "vscode nvm integration",
  "description": "nvmrc integration",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "richard-lopes-ifood"
  },
  "repository": {
    "url": "git@github.com:richard-lopes-ifood/vscode-nvm.git",
    "type": "git"
  },
  "publisher": "richard-lopes-ifood",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:.nvmrc"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": []
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "tslint": "^5.8.0",
    "typescript": "^3.1.4",
    "vsce": "^1.79.5",
    "vscode": "^1.1.25"
  }
}
